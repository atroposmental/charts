{
    "name": "maantje/charts",
    "description": "SVG Charts in PHP",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Maantje\\Charts\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jamie Schouten",
            "email": "j4mie@hey.com"
        }
    ],
    "scripts": {
        "examples": [
            "cd examples && for example in *.php; do php \"$example\" > \"./output/$(basename \"${example%.php}.svg\")\"; done"
        ]
    },
    "require": {
        "laravel/pint": "^1.17"
    },
    "require-dev": {
        "pestphp/pest": "^2.35",
        "phpstan/phpstan": "^1.12"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
